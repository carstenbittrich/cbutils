\mode<presentation>

\definecolor{grblack}{rgb}{0.26,0.26,0.26}
\definecolor{grdarkblue}{rgb}{0.25,0.51,0.7}
\definecolor{grlightblue}{rgb}{0.864,0.93,0.994}

\definecolor{grcolor2}{rgb}{0.2,0.6,0.2}
\definecolor{grcolor3}{rgb}{1.0,0.6,0.2}

\setbeamercolor*{normal text}{fg=grblack,bg=white}
\setbeamercolor*{alerted text}{fg=white,bg=grdarkblue}
\setbeamercolor*{example text}{fg=white,bg=grdarkblue}
\setbeamercolor*{structure}{fg=grdarkblue}

\providecommand*{\beamer@bftext@only}{%
  \relax
  \ifmmode
    \expandafter\beamer@bftext@warning
  \else
    \expandafter\bfseries
  \fi
}
\providecommand*{\beamer@bftext@warning}{%
  \ClassWarning{beamer}
    {Cannot use bold for alerted text in math mode}%
}

\setbeamerfont{alerted text}{series=\beamer@bftext@only}

\setbeamercolor{palette primary}{fg=white,bg=grdarkblue}
\setbeamercolor{palette secondary}{fg=white,bg=grlightblue}
\setbeamercolor{palette tertiary}{fg=white,bg=grlightblue}
\setbeamercolor{palette quaternary}{fg=white,bg=grlightblue}

\setbeamercolor{sidebar}{bg=black!20}

\setbeamercolor{palette sidebar primary}{fg=grblack}
\setbeamercolor{palette sidebar secondary}{fg=grdarkblue}
\setbeamercolor{palette sidebar tertiary}{fg=grlightblue}
\setbeamercolor{palette sidebar quaternary}{fg=black}

\setbeamercolor{item projected}{fg=grdarkblue,bg=grdarkblue}

\setbeamercolor{block title}{use=structure,fg=white,bg=structure.fg}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.bg}
\setbeamercolor{block title example}{use=example text,fg=grdarkblue,bg=grlightblue}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=grlightblue,fg=grdarkblue}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=grlightblue}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=grlightblue}

\setbeamercolor*{titlelike}{parent=structure}

\mode
<all>
